{{> header}}

<h1>Cadastrar-se</h1>

<form method="POST">
	<label>
		Nome:
		<input type="text" name="name" />
	</label>
	<br />
	<label>
		Email:
		<input type="email" name="email" />
	</label>
	<br />
	<label>
		Senha:
		<input type="password" name="password" />
	</label>
	<br />
	<input type="submit" value="Cadastrar" />
</form>



{{> footer}}