{{> header}}

<h2> Editar Post </h2>

<form method="POST">
	<label>
		<input type="text" name="title" value="{{post.title}}" /><br />
	</label>
	<label>
		<textarea name="body">{{post.body}}</textarea><br />
	</label>
	<label>
		Tags:   <br />
	</label>
	<input type="submit" value="Editar >>>" />

</form> 



{{> footer}}