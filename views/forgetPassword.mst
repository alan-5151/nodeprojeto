{{> header}}

<h1>Altere sua senha</h1>

<form method="POST">
	<label>
		Nova senha:
		<input type="password" name="password" />
	</label>
	<br />
	<label>
		Confirmar nova senha:
		<input type="password" name="password-confirm" />
	</label>
	<br />
	<input type="submit" value="Alterar" />
</form>

{{> footer}}